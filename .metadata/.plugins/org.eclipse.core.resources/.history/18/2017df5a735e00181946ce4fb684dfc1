package com.rcdata.robin;

abstract class Animal {
	private String name;

	Animal(String name) {
		this.name = name;
	}

	public abstract void enjoy();
}

abstract class Cat extends Animal {
	private String eyesColor;

	Cat(String n,String c){
		super(n);
		eyesColor=c;
		public void enjoy(){
		System.out.println("miaomiaomiao");
	}
	}
}

